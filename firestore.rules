rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /users/{userId} {
      allow read: if true;
      allow write: if true;
    }
    match /courses/{courseId} {
      allow read: if true;
      allow write: if true;
    }
    match /{document=**} {
      allow read: if true;
      allow write: if true;
    }
  }
}
